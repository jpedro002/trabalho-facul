{
    "name": "Java, Python, and C++",
    "build": {
		"dockerfile": "Dockerfile"
	},
    "extensions": [
        "vscjava.vscode-java-pack",
        "ms-python.python",
        "ms-vscode.cpptools"
    ],
    "settings": {
        "python.pythonPath": "/usr/bin/python3"
    },
    "forwardPorts": [],
    "postCreateCommand": "sudo chown -R vscode: /workspaces",
    "remoteUser": "vscode"
}
